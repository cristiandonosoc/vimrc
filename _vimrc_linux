so ~/vimrc/vimrc.vim
so ~/.vim/vundle_list.vim

" Solarized color scheme enable (put solarized.vim in .vim/colors)
set background=dark
try
  colorscheme solarized
catch
  colorscheme torte
endtry

let g:scratch_autohide = 1
let g:ycm_confirm_extra_conf = 0
" YouCompleteMe
let g:ycm_confirm_extra_conf=0

source ~/.vim/templates.vim
